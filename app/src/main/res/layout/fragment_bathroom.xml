<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BathroomFragment"
    android:background="@drawable/login_box">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-185dp">
        <ImageButton
            android:id="@+id/back_button4"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginBottom="15dp"
            android:adjustViewBounds="true"
            android:background="@color/transparent"
            android:scaleType="fitCenter"
            android:src="@drawable/back_button"/>


        <RelativeLayout
            android:id="@+id/types"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/back_button4"
            android:layout_centerHorizontal="true">


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/bed_content">

                <ImageButton
                    android:id="@+id/chair1"
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="10dp"
                    android:src="@drawable/chair3"
                    android:stateListAnimator="@null"
                    android:scaleType="fitCenter"
                    android:background="@color/transparent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Chair1"
                    android:textColor="@color/brown"
                    android:layout_below="@id/chair1"
                    android:layout_centerInParent="true"
                    android:fontFamily="@font/gmarketsansbold"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="15dp"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/drawer_content"
                android:layout_toRightOf="@+id/bed_content">

                <ImageButton
                    android:id="@+id/chair2"
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="10dp"
                    android:src="@drawable/chair2"
                    android:stateListAnimator="@null"
                    android:scaleType="fitCenter"
                    android:background="@color/transparent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Chair2"
                    android:textColor="@color/brown"
                    android:layout_below="@id/chair2"
                    android:layout_centerInParent="true"
                    android:fontFamily="@font/gmarketsansbold"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="15dp"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/desk_content"
                android:layout_below="@+id/bed_content">
                <ImageButton
                    android:id="@+id/chair3"
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="10dp"
                    android:src="@drawable/chair1"
                    android:stateListAnimator="@null"
                    android:scaleType="fitCenter"
                    android:background="@color/transparent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Chair3"
                    android:textColor="@color/brown"
                    android:layout_below="@id/chair3"
                    android:layout_centerInParent="true"
                    android:fontFamily="@font/gmarketsansbold"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="15dp"/>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/chair_content"
                android:layout_toRightOf="@+id/bed_content"
                android:layout_below="@+id/bed_content">
                <ImageButton
                    android:id="@+id/chair4"
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="10dp"
                    android:src="@drawable/chair4"
                    android:stateListAnimator="@null"
                    android:scaleType="fitCenter"
                    android:background="@color/transparent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Chair4"
                    android:textColor="@color/brown"
                    android:layout_below="@id/chair4"
                    android:layout_centerInParent="true"
                    android:fontFamily="@font/gmarketsansbold"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="15dp"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sofa_content"
                android:layout_below="@+id/desk_content">

                <ImageButton
                    android:id="@+id/chair5"
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="10dp"
                    android:src="@drawable/chair5"
                    android:stateListAnimator="@null"
                    android:scaleType="fitCenter"
                    android:background="@color/transparent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Chair5"
                    android:textColor="@color/brown"
                    android:layout_below="@id/chair5"
                    android:layout_centerInParent="true"
                    android:fontFamily="@font/gmarketsansbold"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="15dp"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/table_content"
                android:layout_below="@+id/desk_content"
                android:layout_toRightOf="@+id/sofa_content">

                <ImageButton
                    android:id="@+id/chair6"
                    android:layout_width="160dp"
                    android:layout_height="160dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="10dp"
                    android:src="@drawable/chair6"
                    android:stateListAnimator="@null"
                    android:scaleType="fitCenter"
                    android:background="@color/transparent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Chair6"
                    android:textColor="@color/brown"
                    android:layout_below="@id/chair6"
                    android:layout_centerInParent="true"
                    android:fontFamily="@font/gmarketsansbold"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="15dp"/>
            </RelativeLayout>

        </RelativeLayout>
    </RelativeLayout>

</FrameLayout>